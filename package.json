{
  "name": "vue",
  "version": "1.0.0",
  "description": "vue template",
  "author": "yingyuk <wuyingyucn@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node server.js",
    "location": "cross-env LOCATION=\"$(git rev-parse --show-toplevel)\" ",
    "build": "npm run build:client && npm run build:server",
    "build:vendor": "cross-env target=vendor node build/build.js ",
    "build:client": "npm run location  cross-env target=client node build/build.js ",
    "build:server": "cross-env target=server node build/build.js ",
    "precommit": "lint-staged",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .eslintignore ",
    "cz": "git-cz"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "es6-promise": "^4.1.1",
    "lodash": "^4.17.4",
    "mint-ui": "^2.2.9",
    "script-loader": "^0.7.2",
    "store": "^2.0.12",
    "swiper": "^4.0.1",
    "vue": "^2.5.2",
    "vue-lazyload": "^1.1.4",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.2",
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.2.0",
    "commitizen": "^2.9.6",
